{
    "compilerOptions": {
        "outDir": "dist",
        "allowJs": true,
        "target": "esnext",
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "noImplicitAny": true,
        "lib": ["dom", "esnext"],
        "esModuleInterop": true,
        // Enabled because the types of utm-latlng are broken
        "skipLibCheck": true
    },
    "include": ["./src/**/*"]
}
